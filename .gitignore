Dockerfile for .jar
# ----- Dockerfile for JAR Deployment -----
FROM openjdk:17-jdk-slim

WORKDIR /app

# Copy JAR file
COPY *.jar app.jar

EXPOSE 8080

# Run the JAR
ENTRYPOINT ["java", "-jar", "app.jar"]


pipeline {
    agent any

    tools {
        jdk 'JAVA_HOME'
        maven 'MAVEN_HOME'
    }

    stages {

        stage('Clone Code') {
            steps {
                git branch: 'master', url: 'https://github.com/gampa-anupama/sample_mavenwebproject.git'
            }
        }

        stage('Build Project') {
            steps {
